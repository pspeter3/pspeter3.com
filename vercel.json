{
    "headers": [
        {
            "source": "/(.*)",
            "headers": [
                {
                    "key": "X-Content-Type-Options",
                    "value": "nosniff"
                },
                {
                    "key": "X-Frame-Options",
                    "value": "deny"
                }
            ]
        }
    ],
    "rewrites": [
        {
            "source": "/rss.xml",
            "destination": "/api/rss"
        },
        {
            "source": "/sitemap.xml",
            "destination": "/api/sitemap"
        }
    ]
}